select 
  n.id,
  n.exception_id
from 
  notification as n 
join 
  truck_shipment_flag as tsf 
on 
  n.exception_id = tsf.id 
where 
  #user_id = 145 and 
  display_to_user = 0 
order by n.id desc ;
#
